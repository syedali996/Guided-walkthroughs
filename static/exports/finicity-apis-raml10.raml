  /v3/customers/{customerId}/transactions:
    uriParameters:
      customerId:
        required: true
        example:
          value: '1005061234'
        displayName: customerId
        description: A customer ID
        type: string
    get:
      displayName: GetAllCustomerTransactions
      description: "Get all transactions available for this customer within the given date range, across all accounts. This service supports paging and sorting by `transactionDate` (or `postedDate` if no transaction date is provided), with a maximum of 1000 transactions per request.\n\nStandard consumer aggregation provides up to 180 days of transactions prior to the date each account was added to the Finicity system. To access older transactions, you must first call the service Load Historic Transactions for Account.\n\nThere is no limit for the size of the window between `fromDate` and `toDate`, however, the maximum number of transactions returned on one page is 1000.\n\n_Supported regions_: ![\U0001F1FA\U0001F1F8](https://flagcdn.com/20x15/us.png)"
      queryParameters:
        fromDate:
          required: true
          example:
            value: 1607450357
          displayName: fromDate
          description: A start date
          type: integer
          format: int64
        toDate:
          required: true
          example:
            value: 1607450357
          displayName: toDate
          description: A end date
          type: integer
          format: int64
        start:
          required: false
          default: '1'
          example:
            value: 1
          displayName: start
          description: Index of the page of results to return
          type: integer
          format: int32
        limit:
          required: false
          default: '25'
          example:
            value: 25
          displayName: limit
          description: Maximum number of results per page
          type: integer
          maximum: 1000
          format: int32
        sort:
          required: false
          default: desc
          example:
            value: desc
          displayName: sort
          description: 'Date sort order: "asc" for ascending, "desc" for descending'
          type: string
        includePending:
          required: false
          default: 'false'
          example:
            value: false
          displayName: includePending
          description: If pending transactions must be included
          type: boolean
      responses:
        '200':
          description: The transactions were successfully retrieved
          body:
            application/json:
              displayName: response200
              description: A list of transactions
              type: Transactions
        '400':
          description: The request was rejected
          body:
            application/json:
              displayName: response400
              description: ''
              type: ErrorMessage
        '401':
          description: The request lacks valid authentication credentials. Check "Finicity-App-Key" or "Finicity-App-Token".
          body:
            application/json:
              displayName: response401
              description: ''
              type: ErrorMessage
        '404':
          description: The resource doesn't exist
